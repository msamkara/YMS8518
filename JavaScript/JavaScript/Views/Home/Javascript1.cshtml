<!doctype html>

<html>
<head>
    <script>

        function IcerigiYazdir() {

            document.write("bu iceriği yazdir");
        }

        function Uyariver() {
            alert("bu uyarıyı ekrana popup olarak cıkart");
        }

        function ConsoleaMesajYazdir() {
            console.log("bu mesajı konsolda goruntule");
        }

        function IcerigiDomaYazdir() {
            document.getElementById("demo").innerHTML = "bu mesaj doma yazdırılıyor";
        }

    </script>

</head>

<body>
    <script>
        IcerigiYazdir();
    </script>
    <hr />
    <input type="button" value="uyarı kutusu cıkart" onclick="UyariVer();" />
    <br />
    <input type="button" value="Console a mesaj yazdır" onclick="ConsoleaMesajYazdir();" />

    <br />
    <input type="button" value="Dom a yazdır" onclick="IcerigiDomaYazdir();" />
    <br />
    <div id="demo">bu icerik</div>


</body>

</html>

